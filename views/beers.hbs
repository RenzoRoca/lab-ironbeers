

{{#each beers}}
<div class="card" style="width: 18rem;">
  <img class="card-img-top" src="{{this.image_url}}" alt="Card image cap">
  <div class="card-body">
    <h4>{{this.name}}</h4>
    <p class="card-text">{{this.description}}</p>
    <p class="card-text">{{this.tagline}}</p>
  </div>
</div>
{{/each}}